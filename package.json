{
  "name": "vscode-rubberduck",
  "displayName": "vscode-rubberduck",
  "description": "",
  "version": "0.0.1",
  "publisher": "arjun",
  "engines": {
    "vscode": "^1.24.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.openSlack"
  ],
  "main": "./out/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.openSlack",
        "title": "Open Slack"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.42",
    "@types/node": "^7.0.43",
    "tslint": "^5.8.0",
    "typescript": "^2.6.1",
    "vscode": "^1.1.6"
  },
  "dependencies": {
    "@slack/client": "^4.3.1"
  }
}
